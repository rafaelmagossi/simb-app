<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="?android:attr/listPreferredItemHeight"
    android:gravity="center_vertical"
    android:layout_alignParentTop="true"
    android:orientation="vertical"
    android:focusable="false" >

    <TextView
        android:id="@+id/textview_diagnostico_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:layout_marginLeft="35dp"
        android:textAllCaps="true"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/textview_diagnostico_datadiag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="20/10/2016"
        android:textSize="14dp"
        android:textAllCaps="true"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginLeft="45dp"
        android:layout_toRightOf="@+id/textview_diagnostico_id"
        android:layout_alignTop="@+id/textview_diagnostico_id" />

    <TextView
        android:id="@+id/textview_diagnostico_resultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CHEIA"
        android:textSize="14dp"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:layout_marginLeft="65dp"
        android:layout_toRightOf="@+id/textview_diagnostico_datadiag"
        android:layout_alignTop="@+id/textview_diagnostico_datadiag" />

   </RelativeLayout>