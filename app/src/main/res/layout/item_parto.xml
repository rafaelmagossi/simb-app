<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="?android:attr/listPreferredItemHeight"
    android:gravity="center_vertical"
    android:layout_alignParentTop="true"
    android:orientation="vertical"

    android:focusable="false" >

    <TextView
        android:id="@+id/textview_parto_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:text="1"
        android:textAllCaps="true"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/textview_parto_dataparto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="20/10/2016"
        android:textSize="14dp"
        android:textAllCaps="true"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/textview_parto_id"
        android:layout_alignTop="@+id/textview_parto_id" />

    <TextView
        android:id="@+id/textview_parto_bovino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aisha 16 DC TE"
        android:textSize="14dp"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/textview_parto_dataparto"
        android:layout_alignTop="@+id/textview_parto_dataparto" />


</RelativeLayout>

